<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">

    <title>{{site.site_name}}</title>
    <meta name="keywords" content="{{site.keywords}}">
    <meta name="description" content="{{site.description}}">
    
    <!-- Main CSS file -->
    <link rel="stylesheet" href="{{common_url('public/home/css/bootstrap.min.css')}}" />
    <link rel="stylesheet" href="{{common_url('public/home/css/owl.carousel.css')}}" />
    <link rel="stylesheet" href="{{common_url('public/home/css/magnific-popup.css')}}" />
    <link rel="stylesheet" href="{{common_url('public/home/css/font-awesome.css')}}" />
    <link rel="stylesheet" href="{{common_url('public/home/css/responsive.css')}}" />
    <link rel="stylesheet" href="{{common_url('public/home/css/style.css')}}" />

    
    <!-- Favicon -->
    <link rel="shortcut icon" href="images/icon/favicon.png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{common_url('public/home/images/icon/apple-touch-icon-144-precomposed.png')}}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{common_url('public/home/images/icon/apple-touch-icon-114-precomposed.png')}}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{common_url('public/home/images/icon/apple-touch-icon-72-precomposed.png')}}">
    <link rel="apple-touch-icon-precomposed" href="{{common_url('public/home/images/icon/apple-touch-icon-57-precomposed.png')}}">
    
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% block style %}{% endblock %}
</head>
<body>

<!-- PRELOADER -->
<div id="st-preloader">
    <div id="pre-status">
        <div class="preload-placeholder"></div>
    </div>
</div>
<!-- /PRELOADER -->

    
    <!-- HEADER -->
    <header id="header">
        <nav class="navbar st-navbar navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#st-navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="logo" href="/"><img src="{{common_url('public/home/images/logo.png')}}" alt=""></a>
                </div>

                <div class="collapse navbar-collapse" id="st-navbar-collapse">
                    <ul class="nav navbar-nav ">
                        {% for nav in navs %}
                            <li><a href="{{nav.url}}">{{nav.name}}</a></li>
                        {% endfor %}
                    </ul>
                </div><!-- /.navbar-collapse -->
            </div><!-- /.container -->
        </nav>
    </header>
    <!-- /HEADER -->

     {% block container %}{% endblock %}

    <!-- FOOTER -->
    <footer id="footer">
        <div class="container">
            <div class="row">
                <!-- SOCIAL ICONS -->
                <div class="col-sm-6 col-sm-push-6 footer-social-icons">
                    <span>Follow us:</span>
                    <a href=""><i class="fa fa-facebook"></i></a>
                    <a href=""><i class="fa fa-twitter"></i></a>
                    <a href=""><i class="fa fa-google-plus"></i></a>
                    <a href=""><i class="fa fa-pinterest-p"></i></a>
                </div>
                <!-- /SOCIAL ICONS -->
                <div class="col-sm-6 col-sm-pull-6 copyright">
                    <p>Copyright &copy; 2016.Company name All rights reserved.<a target="_blank" href="http://sc.chinaz.com/moban/">&#x7F51;&#x9875;&#x6A21;&#x677F;</a></p>
                </div>
            </div>
        </div>
    </footer>
    <!-- /FOOTER -->

    <!-- Scroll-up -->
    <div class="scroll-up">
        <ul><li><a href="#header"><i class="fa fa-angle-up"></i></a></li></ul>
    </div>
    
    <!-- JS -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.min.js')}}"></script><!-- jQuery -->
    <script type="text/javascript" src="{{common_url('public/home/js/bootstrap.min.js')}}"></script><!-- Bootstrap -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.parallax.js')}}"></script><!-- Parallax -->
    <script type="text/javascript" src="{{common_url('public/home/js/smoothscroll.js')}}"></script><!-- Smooth Scroll -->
    <script type="text/javascript" src="{{common_url('public/home/js/masonry.pkgd.min.js')}}"></script><!-- masonry -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.fitvids.js')}}"></script><!-- fitvids -->
    <script type="text/javascript" src="{{common_url('public/home/js/owl.carousel.min.js')}}"></script><!-- Owl-Carousel -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.counterup.min.js')}}"></script><!-- CounterUp -->
    <script type="text/javascript" src="{{common_url('public/home/js/waypoints.min.js')}}"></script><!-- CounterUp -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.isotope.min.js')}}"></script><!-- isotope -->
    <script type="text/javascript" src="{{common_url('public/home/js/jquery.magnific-popup.min.js')}}"></script><!-- magnific-popup -->
    <script type="text/javascript" src="{{common_url('public/home/js/scripts.js')}}"></script><!-- Scripts -->
    {% block script %}

    {% endblock %}

</body>
</html>